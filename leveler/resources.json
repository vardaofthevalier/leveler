{
   "Version": "v1",
   "Resources": [
      {
         "Name": "integration",
         "Usage": "integration",
         "ShortDescription": "Manage an integration with a 3rd party service",
         "LongDescription": "Manage an integration with a 3rd party service.  This enables the system to act on your behalf in order to access 3rd party resources.",
         "SupportedOperations": ["add", "get", "list", "remove"],
         "FromOptions": {
            "Options": [
               {
                  "Name": "name",
                  "Required": ["add", "get", "remove", "update"],
                  "Description": "A descriptive name for the integration"
               }
            ],
            "Subcommands": [
               {
                  "Name": "aws",
                  "Usage": "aws",
                  "ShortDescription": "An integration with AWS",
                  "LongDescription": "An integration with AWS",
                  "Options": [
                     {
                        "Name": "aws_secret_key",
                        "Required": ["add"],
                        "Description": "AWS secret key used to access S3",
                        "Type": "string"
                     },
                     {
                        "Name": "aws_secret_key_id",
                        "Required": ["add"],
                        "Description": "AWS secret key id used to access S3"
                     }
                  ],
                  "ProtobufType": "AWSAccessConfig",
                  "ParentField": "aws"
               },
               {
                  "Name": "nexus",
                  "Usage": "nexus",
                  "ShortDescription": "An integration with Sonatype Nexus",
                  "LongDescription": "An integration with Sonatype Nexus",
                  "Options": [
                     {
                        "Name": "nexus_username",
                        "Required": ["add"],
                        "Description": "Username for accessing Nexus",
                        "Type": "string"
                     },
                     {
                        "Name": "nexus_password",
                        "Required": ["add"],
                        "Description": "Password for accessing Nexus",
                        "Type": "string"
                     },
                     {
                        "Name": "nexus_host",
                        "Required": ["add"],
                        "Description": "The Nexus host to use for this integration",
                        "Type": "string"
                     },
                     {
                        "Name": "nexus_port",
                        "Required": ["add"],
                        "Description": "The port where Nexus is running",
                        "Type": "string"
                     }
                  ],
                  "ProtobufType": "NexusAccessConfig",
                  "ParentField": "nexus"
               },
               {
                  "Name": "bitbucket",
                  "Usage": "bitbucket",
                  "ShortDescription": "An integration with Bitbucket",
                  "LongDescription": "An integration with Bitbucket",
                  "Options": [
                     // TODO: add more here!  All data should map to BitbucketAccessConfig in integration.proto
                  ],
                  "ProtobufType": "BitbucketAccessConfig",
                  "ParentField": "bitbucket"
               },
               {
                  "Name": "github",
                  "Usage": "github",
                  "ShortDescription": "An integration with Github",
                  "LongDescription": "An integration with Github",
                  "Options": [
                     // TODO: add more here! All data should map to GithubAccessConfig in integration.proto
                  ],
                  "ProtobufType": "GithubAccessConfig",
                  "ParentField": "github"
               },
               {
                  "Name": "slack",
                  "Usage": "slack",
                  "ShortDescription": "An integration with Slack",
                  "LongDescription": "An integration with Slack",
                  "Options": [
                     // TODO: add more here! All data should map to SlackAccessConfig in integration.proto
                  ],
                  "ProtobufType": "SlackAccessConfig",
                  "ParentField": "slack"
               }
            ]
         },
         "ProtobufType": "Integration"
      },
      {
         "Name": "repository",
         "Usage": "repository",
         "ShortDescription": "An SCM repository",
         "LongDescription": "An SCM repository",
         "SupportedOperations": ["add", "get", "list", "remove"],
         "Options": [
            // TODO: more here!
         ],
         "ProtobufType": "Repository"
      },
      {
         "Name": "pipeline",
         "Usage": "pipeline",
         "ShortDescription": "A pipeline to run on the cluster",
         "LongDescription": "A pipeline to run on the cluster",
         "SupportedOperations": ["run", "get", "list", "cancel"],
         "FromFile": {
            "MergeOptions": [
               {
                  "Name": "id",
                  "Required": ["get", "cancel"],
                  "Description": "The ID of the pipeline of interest",
                  "Type": "string"
               }
            ],
            "Required": ["run"]
         },
         "ProtobufType": "Pipeline"
      },
      {
         "Name": "job",
         "Usage": "job",
         "ShortDescription": "A pipeline to run on the cluster",
         "LongDescription": "A pipeline to run on the cluster",
         "SupportedOperations": ["get", "list"],
         "FromOptions": {
            "Options": [
               {
                  "Name": "id",
                  "Required": ["get"],
                  "Description": "The ID of the job of interest",
                  "Type": "string"
               },
            ]
         },
         "ProtobufType": "none"
      },
      {
         "Name": "logs",
         "Usage": "logs",
         "ShortDescription": "Get logs for a pipeline or job",
         "LongDescription": "Get logs for a pipeline or job",
         "SupportedOperations": ["get"],
         "FromOptions": {
            "Options": [
               {
                  "Name": "id",
                  "Required": ["get"],
                  "Description": "The ID of the loggable object of interest",
                  "Type": "string"
               },
               {
                  "Name": "type",
                  "Required": ["get"],
                  "Description": "The type of the loggable object of interest",
                  "Type": "string"
               }
            ]
         },
         "ProtobufType": "Loggable"
      }
   ]
}